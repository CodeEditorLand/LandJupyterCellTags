{
	"name": "vscode-jupyter-cell-tags",
	"displayName": "Jupyter Cell Tags",
	"version": "0.0.1",
	"private": false,
	"description": "Jupyter Cell Tags support for VS Code",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandJupyterCellTags#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandJupyterCellTags/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandJupyterCellTags.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üõ¨‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension-node.js",
	"browser": "./out/extension-web.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "webpack --mode none",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "jupyter-cell-tags.removeTag",
				"icon": "$(close)",
				"title": "Remove Cell Tag"
			},
			{
				"command": "jupyter-cell-tags.addTag",
				"icon": "$(add)",
				"title": "Add Cell Tag"
			},
			{
				"command": "jupyter-cell-tags.editTagsInJSON",
				"icon": "$(go-to-file)",
				"title": "Edit Cell Tags (JSON)"
			},
			{
				"command": "jupyter-cell-tags.paramaterize",
				"title": "Mark Cell as Parameters"
			}
		],
		"menus": {
			"notebook/cell/title": [
				{
					"command": "jupyter-cell-tags.addTag",
					"group": "jupytercelltags@1"
				},
				{
					"command": "jupyter-cell-tags.editTagsInJSON",
					"group": "jupytercelltags@2"
				},
				{
					"command": "jupyter-cell-tags.paramaterize",
					"group": "jupytercelltags@2"
				}
			],
			"view/item/context": [
				{
					"command": "jupyter-cell-tags.removeTag",
					"group": "inline",
					"when": "view == cell-tag"
				}
			],
			"view/title": [
				{
					"command": "jupyter-cell-tags.addTag",
					"group": "navigation",
					"when": "view == cell-tag"
				},
				{
					"command": "jupyter-cell-tags.editTagsInJSON",
					"group": "navigation",
					"when": "view == cell-tag"
				}
			]
		},
		"views": {
			"jupyter-variables": [
				{
					"icon": "$(tag)",
					"id": "cell-tag",
					"name": "Cell Tags",
					"type": "tree",
					"visibility": "collapsed",
					"when": "jupyter:showTagsExplorer && jupyter.hasNativeNotebookOrInteractiveWindowOpen"
				}
			]
		},
		"viewsWelcome": [
			{
				"contents": "No tags found for the selected cell. Use the [Add Cell Tag](command:jupyter-cell-tags.addTag) command to add tags.",
				"view": "cell-tag"
			}
		]
	},
	"activationEvents": [
		"onNotebook:jupyter-notebook",
		"onCommand:jupyter-cell-tags.removeTag",
		"onCommand:jupyter-cell-tags.addTag",
		"onCommand:jupyter-cell-tags.editTagsInJSON",
		"onCommand:jupyter-cell-tags.paramaterize"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "8.1.0",
		"@types/node": "20.x",
		"glob": "10.3.12",
		"ts-loader": "9.5.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#ffffff",
		"theme": "light"
	},
	"preview": true
}
