{
	"name": "vscode-jupyter-cell-tags",
	"displayName": "Jupyter Cell Tags",
	"version": "0.0.1",
	"private": false,
	"description": "Jupyter Cell Tags support for VS Code",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension-node.js",
	"browser": "./out/extension-web.js",
	"scripts": {
		"compile": "webpack --mode none",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "webpack --mode none --watch"
	},
	"contributes": {
		"commands": [
			{
				"command": "jupyter-cell-tags.removeTag",
				"icon": "$(close)",
				"title": "Remove Cell Tag"
			},
			{
				"command": "jupyter-cell-tags.addTag",
				"icon": "$(add)",
				"title": "Add Cell Tag"
			},
			{
				"command": "jupyter-cell-tags.editTagsInJSON",
				"icon": "$(go-to-file)",
				"title": "Edit Cell Tags (JSON)"
			},
			{
				"command": "jupyter-cell-tags.paramaterize",
				"title": "Mark Cell as Parameters"
			}
		],
		"menus": {
			"notebook/cell/title": [
				{
					"command": "jupyter-cell-tags.addTag",
					"group": "jupytercelltags@1"
				},
				{
					"command": "jupyter-cell-tags.editTagsInJSON",
					"group": "jupytercelltags@2"
				},
				{
					"command": "jupyter-cell-tags.paramaterize",
					"group": "jupytercelltags@2"
				}
			],
			"view/item/context": [
				{
					"command": "jupyter-cell-tags.removeTag",
					"group": "inline",
					"when": "view == cell-tag"
				}
			],
			"view/title": [
				{
					"command": "jupyter-cell-tags.addTag",
					"group": "navigation",
					"when": "view == cell-tag"
				},
				{
					"command": "jupyter-cell-tags.editTagsInJSON",
					"group": "navigation",
					"when": "view == cell-tag"
				}
			]
		},
		"views": {
			"jupyter-variables": [
				{
					"icon": "$(tag)",
					"id": "cell-tag",
					"name": "Cell Tags",
					"type": "tree",
					"visibility": "collapsed",
					"when": "jupyter:showTagsExplorer && jupyter.hasNativeNotebookOrInteractiveWindowOpen"
				}
			]
		},
		"viewsWelcome": [
			{
				"contents": "No tags found for the selected cell. Use the [Add Cell Tag](command:jupyter-cell-tags.addTag) command to add tags.",
				"view": "cell-tag"
			}
		]
	},
	"activationEvents": [
		"onNotebook:jupyter-notebook",
		"onCommand:jupyter-cell-tags.removeTag",
		"onCommand:jupyter-cell-tags.addTag",
		"onCommand:jupyter-cell-tags.editTagsInJSON",
		"onCommand:jupyter-cell-tags.paramaterize"
	],
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.72.0",
		"glob": "^7.1.7",
		"mocha": "^10.0.0",
		"ts-loader": "^9.1.1",
		"typescript": "^4.3.2",
		"typescript-esbuild": "0.4.5",
		"vscode-test": "^1.5.2",
		"webpack": "^5.36.2",
		"webpack-cli": "^4.6.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#ffffff",
		"theme": "light"
	},
	"preview": true
}
